# Database URLs
# Use Supabase or your own PostgreSQL instance
# DATABASE_URL is used by the app (through PgBouncer connection pool)
# DIRECT_URL is used by Prisma migrations (direct connection)
DATABASE_URL="postgresql://USER:PASSWORD@HOST:6543/postgres?pgbouncer=true&schema=public&sslmode=require"
DIRECT_URL="postgresql://USER:PASSWORD@HOST:5432/postgres?schema=public&sslmode=require"

# Hostaway (API integration ready, but currently mock data is used)
HOSTAWAY_ACCOUNT_ID=""
HOSTAWAY_API_KEY=""

# Path to local mock data file (used during seeding)
HOSTAWAY_JSON_PATH="./data/hostaway-sample.json"

# Next.js App URL (used for links)
NEXT_PUBLIC_APP_URL="http://localhost:3000"
